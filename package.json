{
  "name": "electionguard-javascript-api",
  "version": "1.0.0",
  "description": "JavaScript implementation of ElectionGuard cryptographic protocol with post-quantum security",
  "main": "api.js",
  "scripts": {
    "start": "node api.js",
    "dev": "nodemon api.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:api": "node test-api.js",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t electionguard-js-api .",
    "docker:run": "docker run -p 3000:3000 electionguard-js-api",
    "docker:compose": "docker-compose up -d",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:compose:down": "docker-compose down"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "crypto": "^1.0.1",
    "node-forge": "^1.3.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "big-integer": "^1.6.51",
    "jsbn": "^1.1.0",
    "crypto-js": "^4.2.0",
    "node-rsa": "^1.1.1",
    "elliptic": "^6.5.4",
    "randombytes": "^2.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "axios": "^1.5.0",
    "supertest": "^6.3.3",
    "@types/node": "^20.8.0",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3"
  },
  "keywords": [
    "electionguard",
    "cryptography",
    "post-quantum",
    "election",
    "voting",
    "security",
    "verifiable",
    "privacy"
  ],
  "author": "ElectionGuard Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
